{
  "buildCommand": "cd webui-react && npm install && npm run build",
  "outputDirectory": "webui-react/dist",
  "installCommand": "cd webui-react && npm install",
  "build": {
    "env": {
      "VITE_API_URL": "https://your-backend-url.vercel.app/api"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
