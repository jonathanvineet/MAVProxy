{
  "version": 2,
  "builds": [
    {
      "src": "index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.py"
    },
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.12"
  }
}
